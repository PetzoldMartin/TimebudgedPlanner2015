<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" version="3.1">
  <display-name>timebudgetplanning</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    
  </welcome-file-list>
  <!-- bootsrap glyphs -->
   <mime-mapping>
        <extension>embedded-opentype</extension>
        <mime-type>bootstrap/fonts/glyphicons-halflings-regular.eot</mime-type>
    </mime-mapping>
     <mime-mapping>
        <extension>woff2</extension>
        <mime-type>bootstrap/fonts/glyphicons-halflings-regular.woff2</mime-type>
    </mime-mapping>
     <mime-mapping>
        <extension>woff</extension>
        <mime-type>bootstrap/fonts/glyphicons-halflings-regular.woff</mime-type>
    </mime-mapping>
     <mime-mapping>
        <extension>truetype</extension>
        <mime-type>bootstrap/fonts/glyphicons-halflings-regular.truetype</mime-type>
    </mime-mapping>
     <mime-mapping>
        <extension>svg</extension>
        <mime-type>bootstrap/fonts/glyphicons-halflings-regular.svg</mime-type>
    </mime-mapping>
</web-app>